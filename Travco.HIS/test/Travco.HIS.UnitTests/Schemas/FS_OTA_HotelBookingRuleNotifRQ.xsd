													<xs:attributeGroup ref="CurrencyCodeGroup"/>
												</xs:extension>
											</xs:complexContent>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="CancelPenalties" type="CancelPenaltiesType" minOccurs="0">
